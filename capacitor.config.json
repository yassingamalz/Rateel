{
    "appId": "com.novavista.tajweed",
    "appName": "tajweed-app",
    "bundledWebApp": {
      "htmlDir": "dist/tajweed-app",
      "jsDir": "dist/tajweed-app/browser"
    },
    "plugins": {
      "SplashScreen": {
        "launchShowDuration": 3000,
        "launchAutoHide": true,
        "backgroundColor": "#1B4332",
